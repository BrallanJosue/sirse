{
    "builds": [
      {
        "src": "sirse_api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "sirse-admin-panel/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "sirse_api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "sirse-admin-panel/$1"
      },
      {
        "src": "/",
        "dest": "sirse-admin-panel/index.html"
      }
    ]
  }
  